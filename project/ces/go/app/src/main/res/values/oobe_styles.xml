<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="PermissionCheckBox">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@drawable/selector_text_permission_checkbox</item>
        <item name="android:textSize" tools:ignore="SpUsage">16dp</item>
        <item name="android:paddingLeft">14dp</item>
        <item name="android:button">@drawable/selector_kika_permission_checkbox</item>
    </style>

    <style name="PermissionUnderline">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginTop">14dp</item>
        <item name="android:layout_marginBottom">14dp</item>
        <item name="android:background">#8e94a9</item>
    </style>

</resources>